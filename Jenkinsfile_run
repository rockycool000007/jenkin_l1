pipeline {
    agent any
    stages {
        stage("Retrive Artifact") {
            steps {
                copyArtifacts(projectName: 'compile-java-program');
            }
        }  
        stage("Run the code") {
            steps {
                sh "'ls' -l"
                sh "'java' JavaProgram"
            }
        }    
    }
    post {
        always {
            cleanWs()
        }
    }
}